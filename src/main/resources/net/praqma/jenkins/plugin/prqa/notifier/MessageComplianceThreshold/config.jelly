<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout"
         xmlns:t="/lib/hudson" xmlns:f="/lib/form">
    
    <f:entry title="${%Message threshold level}" field="thresholdLevel">
        <f:select/>
    </f:entry>
    <f:entry title="${%Max messages within threshold}" field="value" >
        <f:textbox/>
    </f:entry>
    <f:entry title="${%Continous improvement}" field="improvement" >
        <f:checkbox/>
    </f:entry>
    
</j:jelly>